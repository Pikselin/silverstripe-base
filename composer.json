{
    "name": "pikselin/base",
    "type": "silverstripe-vendormodule",
    "description": "Add core config and options",
    "keywords": [
        "silverstripe",
        "admin"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ash Etchell",
            "homepage": "https://github.com/ornatipinnis",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4",
        "innoweb/silverstripe-social-metadata": "dev-master",
        "silverstripe/googlesitemaps": "^2.2",
        "gorriecoe/silverstripe-menu": "^2.0",
        "ryanpotter/silverstripe-cms-theme": "^3.4",
        "dnadesign/silverstripe-elemental": "^4",
        "pikselin/silverstripe-admin-edit-link": "dev-main"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "Pikselin\\Extensions\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

