{
    "name": "pikselin/silverstripe-base",
    "type": "silverstripe-vendormodule",
    "description": "Add core config and options",
    "keywords": [
        "silverstripe",
        "admin"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pikselin dev",
            "homepage": "https://github.com/Pikselin",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4",
        "gorriecoe/silverstripe-menu": "^2.0",
        "plastyk/dashboard": "^2.0",
        "ryanpotter/silverstripe-cms-theme": "dev-master#44a15b6a071bd36c57d0a72ecb0e3d141e5a86d0",
        "heyday/silverstripe-responsive-images": "^2.0",
        "silverstripe/redirectedurls": "^2.2",
        "innoweb/silverstripe-page-icons": "^2.4",
        "stevie-mayhew/silverstripe-svg": "^2.2",
        "jonom/focuspoint": "^4.0",
        "axllent/silverstripe-email-obfuscator": "^2.1",
        "silverstripe/mimevalidator": "^2.5",
        "silverstripe/mfa": "^4.8",
        "silverstripe/spellcheck": "^2.5",
        "axllent/silverstripe-trailing-slash": "^2.2",
        "heyday/silverstripe-hashpath": "^3.0",
        "silverstripe/crontask": "^2.6",
        "toastnz/favicon": "dev-main"
        
    },
    "require-dev": {
        "dnadesign/silverstripe-populate": "^2.2"
    },
    "config": {
        "process-timeout": 600
    },


    "extra": {
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "Pikselin\\Extensions\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

