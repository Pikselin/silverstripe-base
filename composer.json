{
    "name": "pikselin/silverstripe-base",
    "type": "silverstripe-vendormodule",
    "description": "Add core config and options",
    "keywords": [
        "silverstripe",
        "admin"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pikselin dev",
            "homepage": "https://github.com/Pikselin",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4",
        "gorriecoe/silverstripe-menu": "^2.0"
        
    },
    "require-dev": {
        "dnadesign/silverstripe-populate": "^2.2"
    },
    "config": {
        "process-timeout": 600
    },


    "extra": {
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "Pikselin\\Extensions\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

