{
    "name": "pikselin/silverstripe-base",
    "type": "silverstripe-vendormodule",
    "description": "Add core config and options",
    "keywords": [
        "silverstripe",
        "admin"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pikselin dev",
            "homepage": "https://github.com/Pikselin",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/cms": "^5",
        "silverstripe/framework": "^5",

        "purplespider/asset-alt-text": "dev-master",
        "heyday/silverstripe-responsive-images": "dev-master",
        "jonom/focuspoint": "dev-master",
        "stevie-mayhew/silverstripe-svg": "dev-master",
        "silverstripe/redirectedurls": "dev-master",
        "tractorcow/silverstripe-robots": "dev-master",
        "colymba/gridfield-bulk-editing-tools": "dev-master",
        "symbiote/silverstripe-gridfieldextensions": "^4",
        "axllent/silverstripe-email-obfuscator": "dev-master",
        "silverstripe/mimevalidator": "^3",
        "silverstripe/mfa": "^5",
        "ryanpotter/silverstripe-cms-theme": "dev-master",
        "jonom/silverstripe-text-target-length": "dev-master",
        "lekoala/silverstripe-cms-actions": "dev-master",
        "axllent/silverstripe-trailing-slash": "dev-master",
        "heyday/silverstripe-hashpath": "dev-main",
        "undefinedoffset/sortablegridfield": "dev-master",

        "silverstripe/crontask": "^3",
        "plastyk/dashboard": "dev-master",
        "silverstripe/sharedraftcontent": "^3",
        "silverstripe/contentreview": "^5",
        "silverstripe/environmentcheck": "^3"
    },
    "require-dev": {
        "dnadesign/silverstripe-populate": "dev-master"
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "expose": [
            "client"
        ],
        "project-files-installed": [
            ".editorconfig",
            ".php-cs-fixer.php",
            "app/.htaccess",
            "app/_config.php",
            "app/_config/dev.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php",
            "phpstan.neon",
            "themes/pikselin/composer.json",
            "themes/pikselin/package.json",
            "themes/pikselin/webpack.mix.js"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "autoload": {
        "psr-4": {
            "Pikselin\\Extensions\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
