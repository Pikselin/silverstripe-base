{
    "name": "pikselin/silverstripe-base",
    "type": "silverstripe-vendormodule",
    "description": "Add core config and options",
    "keywords": [
        "silverstripe",
        "admin"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pikselin dev",
            "homepage": "https://github.com/Pikselin",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4",
        "gorriecoe/silverstripe-menu": "^2.0",
        "purplespider/asset-alt-text": "^1.1",
        "heyday/silverstripe-responsive-images": "^2.0",
        "jonom/focuspoint": "^4.0",
        "stevie-mayhew/silverstripe-svg": "^2.2",
        "silverstripe/redirectedurls": "^2.2",
        "tractorcow/silverstripe-robots": "^4.0",
        "colymba/gridfield-bulk-editing-tools": "^3.1",
        "symbiote/silverstripe-gridfieldextensions": "^3.6",
        "axllent/silverstripe-email-obfuscator": "^2.1",
        "silverstripe/mimevalidator": "^2.5",
        "silverstripe/mfa": "^4.8",
        "silverstripe/docstation": "dev-main#a8b42061437a972a8f311fb8a8fcd53d4dde092b",
        "ryanpotter/silverstripe-cms-theme": "dev-master#44a15b6a071bd36c57d0a72ecb0e3d141e5a86d0",
        "jonom/silverstripe-text-target-length": "^2.0",
        "lekoala/silverstripe-cms-actions": "^1.4",
        "axllent/silverstripe-trailing-slash": "^2.2",
        "heyday/silverstripe-hashpath": "^3.0",
        "undefinedoffset/sortablegridfield": "^2.2",
        "quinninteractive/silverstripe-seo": "^1.1",
        "silverstripe/crontask": "^2.6",
        "plastyk/dashboard": "^2.0",
        "silverstripe/sharedraftcontent": "^2.8|^3",
        "silverstripe/contentreview": "^4.7",
        "silverstripe/environmentcheck": "^2.6|^3",
        "bringyourownideas/silverstripe-maintenance": "^2.7",
        "bringyourownideas/silverstripe-composer-update-checker": "^3.1",
        "firesphere/cspheaders": "^1.4",
        "pikselin/module-helpers": "*"
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:pikselin/silverstripe-elemental"
        },
        {
            "type": "github",
            "url": "git@github.com:Firesphere/potential-doodle.git"
        },
        {
            "type": "github",
            "url": "git@github.com:Pikselin/coding-standards.git"
        }
    ],
    "require-dev": {
        "dnadesign/silverstripe-populate": "^2.2",
        "pikselin/coding-standards": "dev-main"
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "expose": [
            "client"
        ],
        "project-files-installed": [
            ".editorconfig",
            ".php-cs-fixer.php",
            "app/.htaccess",
            "app/_config.php",
            "app/_config/dev.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php",
            "phpstan.neon",
            "themes/pikselin/composer.json",
            "themes/pikselin/package.json",
            "themes/pikselin/webpack.mix.js"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "autoload": {
        "psr-4": {
            "Pikselin\\Extensions\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
